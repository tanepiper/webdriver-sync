{
  "name": "webdriver-sync",
  "version": "0.18.0",
  "description": "Selenium testing without nested callbacks or promises!",
  "main": "src/webdriver-sync.js",
  "scripts": {
    "lint-src": "node_modules/.bin/jshint --config .jshintrc src",
    "lint-test": "node_modules/.bin/jshint --config .test-jshintrc test",
    "mocha": "node_modules/.bin/mocha -r should --no-timeouts test/*.js test/classes/*.js test/enums/*.js test/interfaces/*.js",
    "test": "npm run lint-src && npm run lint-test && npm run mocha"
  },
  "dependencies": {
    "async": "~2.1.5",
    "java": "0.8.0",
    "selenium-binaries": "SkReD/selenium-binaries"
  },
  "devDependencies": {
    "jshint": "2.9.4",
    "mocha": "^3.2.0",
    "proxyquire": "^1.7.11",
    "should": "*",
    "sinon": "^2.0.0-pre.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/webdriver-sync.git"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "testing",
    "sync",
    "synchronous",
    "api",
    "chrome",
    "safari",
    "internet explorer",
    "explorer",
    "internet",
    "ie",
    "driver",
    "node",
    "javascript"
  ],
  "author": "Joseph Spencer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdevel/webdriver-sync/issues"
  }
}
